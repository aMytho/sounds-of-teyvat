{
    "name": "sounds-of-teyvat",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "prisma:dev:deploy": "npx prisma migrate dev --schema libs/models/prisma/schema.prisma",
        "prisma:dev:generate": "npx prisma generate --schema libs/models/prisma/schema.prisma",
        "db:dev:rm": "docker compose rm dev-db -s -f -v",
        "db:dev:up": "docker compose up dev-db -d",
        "db:dev:restart": "npm run db:dev:rm && npm run db:dev:up && npm run prisma:dev:deploy",
        "db:dev:view": "npx prisma studio --schema libs/models/prisma/schema.prisma",
        "prisma:test:deploy": "dotenv -e .env.test -- npx prisma migrate deploy --schema libs/models/prisma/schema.prisma",
        "db:test:rm": "docker compose rm test-db -s -f -v",
        "db:test:up": "docker compose up test-db -d",
        "db:test:restart": "npm run db:test:rm && npm run db:test:up && npm run prisma:test:deploy",
        "ng": "nx",
        "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
        "start": "nx serve",
        "start:api": "nx serve --project api",
        "start:web": "nx serve --project web",
        "build": "nx build",
        "test": "nx test",
        "test:api:e2e": "dotenv -e .env.test -- nx test --project api --testFile apps/api/src/test/e2e.spec.ts --watch"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.3.0",
        "@angular/common": "~13.3.0",
        "@angular/compiler": "~13.3.0",
        "@angular/core": "~13.3.0",
        "@angular/forms": "~13.3.0",
        "@angular/platform-browser": "~13.3.0",
        "@angular/platform-browser-dynamic": "~13.3.0",
        "@angular/router": "~13.3.0",
        "@nestjs/common": "^8.0.0",
        "@nestjs/config": "^2.0.0",
        "@nestjs/core": "^8.0.0",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/passport": "^8.2.1",
        "@nestjs/platform-express": "^8.0.0",
        "@nrwl/angular": "13.10.1",
        "@prisma/client": "^3.12.0",
        "argon2": "^0.28.5",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "passport": "^0.5.2",
        "passport-jwt": "^4.0.0",
        "prisma": "^3.12.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "~7.4.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.3.0",
        "@angular-eslint/eslint-plugin": "~13.1.0",
        "@angular-eslint/eslint-plugin-template": "~13.1.0",
        "@angular-eslint/template-parser": "~13.1.0",
        "@angular/cli": "~13.3.0",
        "@angular/compiler-cli": "~13.3.0",
        "@angular/language-service": "~13.3.0",
        "@nestjs/schematics": "^8.0.0",
        "@nestjs/testing": "^8.0.0",
        "@nrwl/cli": "13.10.1",
        "@nrwl/cypress": "13.10.1",
        "@nrwl/eslint-plugin-nx": "13.10.1",
        "@nrwl/jest": "13.10.1",
        "@nrwl/linter": "13.10.1",
        "@nrwl/nest": "13.10.1",
        "@nrwl/node": "13.10.1",
        "@nrwl/workspace": "13.10.1",
        "@types/jest": "27.0.2",
        "@types/node": "16.11.7",
        "@types/passport-jwt": "^3.0.6",
        "@typescript-eslint/eslint-plugin": "~5.18.0",
        "@typescript-eslint/parser": "~5.18.0",
        "autoprefixer": "^10.4.0",
        "cypress": "^9.1.0",
        "dotenv-cli": "^5.1.0",
        "eslint": "~8.12.0",
        "eslint-config-prettier": "8.1.0",
        "eslint-plugin-cypress": "^2.10.3",
        "jest": "27.2.3",
        "jest-preset-angular": "11.1.1",
        "nx": "13.10.1",
        "pactum": "^3.1.6",
        "postcss": "^8.4.5",
        "prettier": "^2.5.1",
        "tailwindcss": "^3.0.2",
        "ts-jest": "27.0.5",
        "typescript": "~4.6.2"
    }
}
